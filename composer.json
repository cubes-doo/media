{
    "name": "cubes-doo/media",
    "description": "Media library for interaction with Youtube and Vimeo web media Api's.",
    "keywords": ["youtube-api", "youtube php", "composer", "vimeo php", "vimeo-php"],
    "homepage": "https://github.com/cubes-doo/media",
    "type": "library",
    "license": "GPL",
    "authors": [
        {
            "name": "Djordje Stojiljkovic",
            "email": "djordje.stojiljkovic@cubes.rs"
        }
    ],
    "require": {
        "php": ">=5.4",
        "vimeo/vimeo-api": "*",
        "google/apiclient": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Cubes\\Media\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload-dev": {
        "psr-4": {
          "Cubes\\Media\\Tests\\": "tests/Media/"
        }
    }

}

